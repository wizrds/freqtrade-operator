{
    "name": "freqtrade-operator",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "..",
		"args": {
			"VARIANT": "1.90_3.12"
		}
    },
	"customizations": {
		"vscode": {
			"extensions": [
				"rust-lang.rust-analyzer"
			]
		}
	},
	"postCreateCommand": "pre-commit install --install-hooks && rustc --version && python --version",
	"remoteUser": "dev"
}